           RowBox[{"i", "=", "1"}], ",", 
           RowBox[{"1", "<=", "i", "<=", 
            RowBox[{"2", "*", 
             RowBox[{"(", 
              RowBox[{"-", "S"}], ")"}]}]}], ",", 
           RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"output", "=", 
             RowBox[{"Simplify", "[", 
              RowBox[{
               RowBox[{"(", 
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{
                   RowBox[{"L0", "[", "output", "]"}], "+", 
                   RowBox[{
                    RowBox[{"(", 
                    RowBox[{"-", "S"}], ")"}], "*", 
                    RowBox[{"Cot", "[", "\[Theta]", "]"}], "*", "output"}]}], 
                  ")"}], "/", 
