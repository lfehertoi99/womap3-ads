Cell[BoxData[{
 RowBox[{
  RowBox[{"\[CapitalDelta]", "[", "r_", "]"}], ":=", 
  RowBox[{"r", "*", 
   RowBox[{"(", 
    RowBox[{"r", "-", 
     RowBox[{"2", "M"}]}], ")"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"V", "[", "r_", "]"}], ":=", 
  RowBox[{
   RowBox[{"4", "I", "*", "s", "*", "\[Omega]", "*", "r"}], "+", "s", "-", 
   "\[Lambda]", "+", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{"\[Omega]", "*", 
         RowBox[{"r", "^", "2"}]}], ")"}], "^", "2"}], "-", 
      RowBox[{"2", "I", "*", "\[Omega]", "*", 
       RowBox[{"r", "^", "2"}], "s", 
       RowBox[{"(", 
        RowBox[{"r", "-", "M"}], ")"}]}]}], ")"}], "/", 
    RowBox[{"\[CapitalDelta]", "[", "r", "]"}]}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"RadTeuk", "[", "f_", "]"}], ":=", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"\[CapitalDelta]", "[", "r", "]"}], "^", 
      RowBox[{"(", 
       RowBox[{"-", "s"}], ")"}]}], "*", 
     RowBox[{"D", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"\[CapitalDelta]", "[", "r", "]"}], "^", 
         RowBox[{"(", 
          RowBox[{"1", "+", "s"}], ")"}]}], "*", 
        RowBox[{"D", "[", 
         RowBox[{"f", ",", "r"}], "]"}]}], ",", "r"}], "]"}]}], "+", 
    RowBox[{
     RowBox[{"V", "[", "r", "]"}], "*", "f"}]}]}], "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"D0", "=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"D", "[", 
       RowBox[{"#", ",", "r"}], "]"}], "-", 
      RowBox[{"I", "*", "\[Omega]", "*", 
       RowBox[{
        RowBox[{"r", "^", "2"}], "/", 
        RowBox[{"\[CapitalDelta]", "[", "r", "]"}]}], "*", "#"}]}], ")"}], 
    "&"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"D0dag", "=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"D", "[", 
       RowBox[{"#", ",", "r"}], "]"}], "+", 
      RowBox[{"I", "*", "\[Omega]", "*", 
       RowBox[{
        RowBox[{"r", "^", "2"}], "/", 
        RowBox[{"\[CapitalDelta]", "[", "r", "]"}]}], "*", "#"}]}], ")"}], 
    "&"}]}], ";"}]}], "Input",
