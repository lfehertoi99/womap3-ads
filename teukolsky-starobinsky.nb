     RowBox[{"f", ",", " ", 
      RowBox[{"{", 
       RowBox[{"\[Theta]", ",", "2"}], "}"}]}], "]"}], "+", 
    RowBox[{
     RowBox[{"Cot", "[", "\[Theta]", "]"}], "*", 
     RowBox[{"D", "[", 
      RowBox[{"f", ",", " ", "\[Theta]"}], "]"}]}], "-", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"m", "-", 
            RowBox[{"s", "*", 
             RowBox[{"Cos", "[", "\[Theta]", "]"}]}]}], ")"}], "^", "2"}], 
         ")"}], "/", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"Sin", "[", "\[Theta]", "]"}], "^", "2"}], ")"}]}], "-", 
       "\[Lambda]"}], ")"}], "*", "f"}]}]}], "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"L0", "=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"D", "[", 
       RowBox[{"#", ",", "\[Theta]"}], "]"}], "+", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"m", "/", 
         RowBox[{"Sin", "[", "\[Theta]", "]"}]}], ")"}], "*", "#"}]}], ")"}], 
    "&"}]}], ";"}], "\[IndentingNewLine]", 
