  3.9717045867397747`*^9, 
  3.971704595828903*^9}},ExpressionUUID->"6ca1e2b6-10f2-41c7-8e5c-\
d61ef3620d2d"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"AngTeukPOS", "[", "f_", "]"}], ":=", 
  RowBox[{
   RowBox[{"D", "[", 
    RowBox[{"f", ",", " ", 
     RowBox[{"{", 
      RowBox[{"\[Theta]", ",", "2"}], "}"}]}], "]"}], "+", 
   RowBox[{
    RowBox[{"Cot", "[", "\[Theta]", "]"}], "*", 
    RowBox[{"D", "[", 
     RowBox[{"f", ",", " ", "\[Theta]"}], "]"}]}], "-", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"m", "+", 
           RowBox[{"s", "*", 
            RowBox[{"Cos", "[", "\[Theta]", "]"}]}]}], ")"}], "^", "2"}], 
        ")"}], "/", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"Sin", "[", "\[Theta]", "]"}], "^", "2"}], ")"}]}], "-", 
      "\[Lambda]"}], ")"}], "*", "f"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"AngTeukNEG", "[", "f_", "]"}], ":=", 
   RowBox[{
