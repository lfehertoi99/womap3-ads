Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"f1", "[", "x_", "]"}], ":=", 
   RowBox[{"Cot", "[", "x", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"f2", "[", "x_", "]"}], ":=", 
   RowBox[{
    FractionBox[
     RowBox[{"-", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"m", "+", 
         RowBox[{"s", "*", 
          RowBox[{"Cos", "[", "x", "]"}]}]}], ")"}], "2"]}], 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"Sin", "[", "x", "]"}], ")"}], "2"]], "+", 
    RowBox[{
     SuperscriptBox["\[Nu]", "2"], "*", 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"Cos", "[", "x", "]"}], ")"}], "2"]}], "-", 
    RowBox[{"2", "*", "\[Nu]", "*", "s", "*", 
     RowBox[{"Cos", "[", "x", "]"}]}], "+", "\[Lambda]"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.971689932153806*^9, 3.9716899635177326`*^9}, {
  3.971690022934559*^9, 3.9716900291690483`*^9}, {3.9716900855743475`*^9, 
