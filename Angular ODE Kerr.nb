Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"A", "[", "0", "]"}], "=", "A0"}], ";"}], "  ", 
  RowBox[{"(*", "free", "*)"}]}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"A", "[", 
    RowBox[{"n_Integer", "?", "Positive"}], "]"}], ":=", 
   RowBox[{
    RowBox[{"A", "[", "n", "]"}], "=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", "sum", "}"}], ",", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Q", "[", 
           RowBox[{"\[Alpha]", "+", "n"}], "]"}], "===", "0"}], ",", 
         RowBox[{
          RowBox[{"Message", "[", 
           RowBox[{
            RowBox[{"PowerSeries", "::", "sing"}], ",", "n"}], "]"}], ";", 
          RowBox[{"Return", "[", "$Failed", "]"}]}]}], "]"}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"sum", "=", 
        RowBox[{"Sum", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{
              RowBox[{"(", 
               RowBox[{"\[Alpha]", "+", "j"}], ")"}], " ", 
              RowBox[{"c1", "[", 
               RowBox[{"n", "-", "j"}], "]"}]}], "+", 
             RowBox[{"c2", "[", 
              RowBox[{"n", "-", "j"}], "]"}]}], ")"}], " ", 
           RowBox[{"A", "[", "j", "]"}]}], ",", 
          RowBox[{"{", 
           RowBox[{"j", ",", "0", ",", 
            RowBox[{"n", "-", "1"}]}], "}"}]}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"Simplify", "[", 
        RowBox[{
         RowBox[{"-", "sum"}], "/", 
         RowBox[{"Q", "[", 
          RowBox[{"\[Alpha]", "+", "n"}], "]"}]}], "]"}]}]}], "]"}]}]}], 
  ";"}]}], "Input",
 CellChangeTimes->{{3.9723159715238333`*^9, 3.9723159791954937`*^9}, {
  3.972387259405428*^9, 3.972387261327312*^9}, {3.972448482856445*^9, 
  3.972448483872388*^9}, {3.9724485201225624`*^9, 3.972448520935996*^9}, {
  3.973053611497573*^9, 3.9730536176080017`*^9}, {3.973054116615139*^9, 
  3.9730541184443464`*^9}, {3.973054185018429*^9, 3.9730541867997227`*^9}},
 Background->RGBColor[
