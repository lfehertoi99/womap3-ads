     RowBox[{"(", 
      RowBox[{
       RowBox[{"\[CapitalDelta]", "[", "\[Theta]", "]"}], "*", 
       RowBox[{"Sin", "[", "\[Theta]", "]"}]}], ")"}]], "*", 
