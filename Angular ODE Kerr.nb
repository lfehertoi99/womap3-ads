Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       SubscriptBox[
        StyleBox["f", "TI"], 
        RowBox[{"1", ",", 
          StyleBox["j", "TI"]}]], TraditionalForm], "errors" -> {}, "input" -> 
