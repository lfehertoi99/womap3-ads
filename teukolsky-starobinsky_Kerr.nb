Cell[BoxData[
 RowBox[{
  RowBox[{"TSradPrep", "[", "S_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"output", "=", 
      RowBox[{"RR", "[", "r", "]"}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"S", ">", "0"}], ",", "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"output", "=", 
          RowBox[{
           RowBox[{
            RowBox[{"\[CapitalDelta]", "[", "r", "]"}], "^", "S"}], "*", 
           "output"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"For", "[", 
          RowBox[{
           RowBox[{"i", "=", "1"}], ",", 
           RowBox[{"1", "<=", "i", "<=", 
            RowBox[{"2", "*", "S"}]}], ",", 
           RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"output", "=", 
             RowBox[{"Simplify", "[", 
              RowBox[{
               RowBox[{"D0dag", "[", "output", "]"}], "/.", "RadTeukRule"}], 
              "]"}]}], ";"}]}], "\[IndentingNewLine]", "]"}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"output", " ", "=", 
          RowBox[{"Simplify", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"\[CapitalDelta]", "[", "r", "]"}], "^", "S"}], "*", 
            "output"}], "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"For", "[", 
          RowBox[{
           RowBox[{"i", "=", "1"}], ",", 
           RowBox[{"1", "<=", "i", "<=", 
            RowBox[{"2", "*", "S"}]}], ",", 
           RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"output", "=", 
             RowBox[{"Simplify", "[", 
              RowBox[{
               RowBox[{"D0", "[", "output", "]"}], "/.", "RadTeukRule"}], 
              "]"}]}], ";"}]}], "\[IndentingNewLine]", "]"}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"output", "=", 
          RowBox[{"Simplify", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"\[CapitalDelta]", "[", "r", "]"}], "^", "S"}], "*", 
            "output"}], "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"output", "=", 
          RowBox[{
           RowBox[{"output", "/", 
            RowBox[{
             RowBox[{"\[CapitalDelta]", "[", "r", "]"}], "^", "s"}]}], "/.", 
           RowBox[{"s", "->", "S"}]}]}], ";"}], "\[IndentingNewLine]", "}"}], 
       ",", "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"For", "[", 
          RowBox[{
           RowBox[{"i", "=", "1"}], ",", 
           RowBox[{"1", "<=", "i", "<=", 
            RowBox[{"2", "*", 
             RowBox[{"(", 
              RowBox[{"-", "S"}], ")"}]}]}], ",", 
           RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"output", "=", 
             RowBox[{"Simplify", "[", 
              RowBox[{
               RowBox[{"D0", "[", "output", "]"}], "/.", "RadTeukRule"}], 
              "]"}]}], ";"}]}], "\[IndentingNewLine]", "]"}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"output", " ", "=", 
          RowBox[{"Simplify", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"\[CapitalDelta]", "[", "r", "]"}], "^", 
             RowBox[{"(", 
              RowBox[{"-", "S"}], ")"}]}], "*", "output"}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"For", "[", 
          RowBox[{
           RowBox[{"i", "=", "1"}], ",", 
           RowBox[{"1", "<=", "i", "<=", 
            RowBox[{"2", "*", 
             RowBox[{"(", 
              RowBox[{"-", "S"}], ")"}]}]}], ",", 
           RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"output", "=", 
             RowBox[{"Simplify", "[", 
              RowBox[{
               RowBox[{"D0dag", "[", "output", "]"}], "/.", "RadTeukRule"}], 
              "]"}]}], ";"}]}], "\[IndentingNewLine]", "]"}], ";", 
         "\[IndentingNewLine]", 
