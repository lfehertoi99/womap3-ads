Cell[TextData[{
 "We start defining the Teukolsky angular ODE operator . In this case, we \
will need to treat separately the case with positive and negative spin, so we \
will have two different angular Teukolsky ODE operators ",
 Cell[BoxData[
 "RadTeukPOS"], "InlineCode",ExpressionUUID->
  "86670525-962b-444f-884f-72f8c668fc43"],
 " (for positive spin) and RadTeukNEG (for negative spin). We also introduce \
the and the L_0 and L^{\\dagger}_0 operators (",
 Cell[BoxData[
 "L0"], "InlineCode",ExpressionUUID->"9e764dac-8938-4777-9eba-6926ffb35d03"],
 " an ",
 Cell[BoxData[
 "L0dag"], "InlineCode",ExpressionUUID->
  "9e815c96-ee98-4626-9dd0-74dbbfe36606"],
 StyleBox[" ",
  FontWeight->"Bold"],
 "respectively):"
}], "Text",
 CellChangeTimes->{{3.9717043299530067`*^9, 3.971704491807611*^9}, {
  3.9717045867397747`*^9, 
  3.971704595828903*^9}},ExpressionUUID->"6ca1e2b6-10f2-41c7-8e5c-\
d61ef3620d2d"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"AngTeukPOS", "[", "f_", "]"}], ":=", 
   RowBox[{
    RowBox[{"D", "[", 
     RowBox[{"f", ",", " ", 
      RowBox[{"{", 
       RowBox[{"\[Theta]", ",", "2"}], "}"}]}], "]"}], "+", 
    RowBox[{
     RowBox[{"Cot", "[", "\[Theta]", "]"}], "*", 
     RowBox[{"D", "[", 
      RowBox[{"f", ",", " ", "\[Theta]"}], "]"}]}], "-", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"m", "+", 
            RowBox[{"s", "*", 
             RowBox[{"Cos", "[", "\[Theta]", "]"}]}]}], ")"}], "^", "2"}], 
         ")"}], "/", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"Sin", "[", "\[Theta]", "]"}], "^", "2"}], ")"}]}], "-", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"a", "*", "\[Omega]", "*", 
          RowBox[{"Cos", "[", "\[Theta]", "]"}]}], ")"}], "^", "2"}], "+", 
       RowBox[{"2", "*", "a", "*", "\[Omega]", "*", "s", "*", 
        RowBox[{"Cos", "[", "\[Theta]", "]"}]}], "-", "\[Lambda]"}], ")"}], "*",
      "f"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"AngTeukNEG", "[", "f_", "]"}], ":=", 
    RowBox[{
     RowBox[{"D", "[", 
      RowBox[{"f", ",", " ", 
       RowBox[{"{", 
        RowBox[{"\[Theta]", ",", "2"}], "}"}]}], "]"}], "+", 
     RowBox[{
      RowBox[{"Cot", "[", "\[Theta]", "]"}], "*", 
      RowBox[{"D", "[", 
       RowBox[{"f", ",", " ", "\[Theta]"}], "]"}]}], "-", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"m", "-", 
             RowBox[{"s", "*", 
              RowBox[{"Cos", "[", "\[Theta]", "]"}]}]}], ")"}], "^", "2"}], 
          ")"}], "/", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"Sin", "[", "\[Theta]", "]"}], "^", "2"}], ")"}]}], "-", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"a", "*", "\[Omega]", "*", 
           RowBox[{"Cos", "[", "\[Theta]", "]"}]}], ")"}], "^", "2"}], "-", 
        RowBox[{"2", "*", "a", "*", "\[Omega]", "*", "s", "*", 
         RowBox[{"Cos", "[", "\[Theta]", "]"}]}], "-", "\[Lambda]"}], ")"}], "*",
       "f"}]}]}], ";"}], "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"L0", "=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"D", "[", 
       RowBox[{"#", ",", "\[Theta]"}], "]"}], "+", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"m", "/", 
           RowBox[{"Sin", "[", "\[Theta]", "]"}]}], ")"}], "-", 
         RowBox[{"a", "*", "\[Omega]", "*", 
          RowBox[{"Sin", "[", "\[Theta]", "]"}]}]}], ")"}], "*", "#"}]}], 
     ")"}], "&"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"L0dag", "=", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"D", "[", 
       RowBox[{"#", ",", "\[Theta]"}], "]"}], "-", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"m", "/", 
          RowBox[{"Sin", "[", "\[Theta]", "]"}]}], "-", 
         RowBox[{"a", "*", "\[Omega]", "*", 
          RowBox[{"Sin", "[", "\[Theta]", "]"}]}]}], ")"}], "*", "#"}]}], 
     ")"}], "&"}]}], ";"}], "\[IndentingNewLine]"}], "Input",
 CellChangeTimes->{{3.9713566022621117`*^9, 3.9713566074612627`*^9}, {
