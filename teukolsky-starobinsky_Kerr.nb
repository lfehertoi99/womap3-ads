Cell[BoxData[{
 RowBox[{
  RowBox[{"\[CapitalXi]", ":=", 
   RowBox[{"1", "-", 
    RowBox[{
     RowBox[{"a", "^", "2"}], 
     RowBox[{"k", "^", "2"}]}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"\[CapitalDelta]", "[", "r_", "]"}], ":=", 
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"r", "^", "2"}], "+", 
       RowBox[{"a", "^", "2"}]}], ")"}], 
     RowBox[{"(", 
      RowBox[{"1", "+", 
       RowBox[{
        RowBox[{"r", "^", "2"}], "*", 
        RowBox[{"k", "^", "2"}]}]}], ")"}]}], "-", 
    RowBox[{"2", "M", "*", "r"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"V1", "[", "r_", "]"}], ":=", 
   RowBox[{
    RowBox[{"1", "/", 
     RowBox[{"\[CapitalDelta]", "[", "r", "]"}]}], "*", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       RowBox[{"\[CapitalXi]", "^", "2"}], "*", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"\[Omega]", "*", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"r", "^", "2"}], "+", 
             RowBox[{"a", "^", "2"}]}], ")"}]}], "-", 
          RowBox[{"a", "*", "m"}]}], ")"}], "^", "2"}]}], "-", 
      RowBox[{"I", "*", "s", "*", "\[CapitalXi]", "*", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"\[Omega]", "*", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"r", "^", "2"}], "+", 
            RowBox[{"a", "^", "2"}]}], ")"}]}], "-", 
         RowBox[{"a", "*", "m"}]}], ")"}], "*", 
       RowBox[{"D", "[", 
        RowBox[{
         RowBox[{"\[CapitalDelta]", "[", "r", "]"}], ",", "r"}], "]"}]}]}], 
     ")"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"V2", "[", "r_", "]"}], ":=", 
   RowBox[{
    RowBox[{
    "4", "I", "*", "s", "*", "\[Omega]", "*", "\[CapitalXi]", "*", "r"}], "+", 
    RowBox[{"2", "*", 
     RowBox[{"k", "^", "2"}], 
     RowBox[{"(", 
      RowBox[{"\[Mu]", "+", 
       RowBox[{"3", "s"}], "+", 
       RowBox[{"2", 
        RowBox[{"s", "^", "2"}]}]}], ")"}], 
     RowBox[{"r", "^", "2"}]}], "+", 
    RowBox[{"s", "*", 
     RowBox[{"(", 
      RowBox[{"1", "+", 
       RowBox[{
        RowBox[{"a", "^", "2"}], 
        RowBox[{"k", "^", "2"}]}]}], ")"}]}], "-", "\[Lambda]", "+", 
    RowBox[{"2", 
     RowBox[{"\[CapitalXi]", "^", "2"}], "*", "a", "*", "m", "*", 
     "\[Omega]"}], "-", 
    RowBox[{
     RowBox[{"a", "^", "2"}], "*", 
     RowBox[{"\[CapitalXi]", "^", "2"}], "*", 
     RowBox[{"\[Omega]", "^", "2"}]}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"RadTeuk", "[", "f_", "]"}], ":=", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"\[CapitalDelta]", "[", "r", "]"}], "^", 
       RowBox[{"(", 
        RowBox[{"-", "s"}], ")"}]}], "*", 
      RowBox[{"D", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"\[CapitalDelta]", "[", "r", "]"}], "^", 
          RowBox[{"(", 
           RowBox[{"1", "+", "s"}], ")"}]}], "*", 
         RowBox[{"D", "[", 
          RowBox[{"f", ",", "r"}], "]"}]}], ",", "r"}], "]"}]}], "+", 
