  3.971679990921682*^9, 3.971682337351342*^9, {3.9717049043959913`*^9, 
   3.971704956586442*^9}, {3.9718622956546583`*^9, 
   3.9718623159638166`*^9}},ExpressionUUID->"cc9470dd-ec38-4f18-a1fd-\
be1318c8b9b9"],

Cell[BoxData[
 RowBox[{
  RowBox[{"TSangPrep", "[", "S_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"output", "=", 
      RowBox[{"AA", "[", "\[Theta]", "]"}]}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"S", ">", "0"}], ",", "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"For", "[", 
          RowBox[{
           RowBox[{"i", "=", "1"}], ",", 
           RowBox[{"1", "<=", "i", "<=", 
            RowBox[{"2", "*", "S"}]}], ",", 
           RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"output", "=", 
             RowBox[{"Simplify", "[", 
              RowBox[{
               RowBox[{"(", 
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{
                   RowBox[{"L0", "[", "output", "]"}], "+", 
                   RowBox[{"S", "*", 
                    RowBox[{"Cot", "[", "\[Theta]", "]"}], "*", "output"}]}], 
                  ")"}], "/", 
                 RowBox[{"Sin", "[", "\[Theta]", "]"}]}], ")"}], "/.", 
               "AngTeukPOSRule"}], "]"}]}], ";"}]}], "\[IndentingNewLine]", 
          "]"}], ";", "\[IndentingNewLine]", 
         RowBox[{"output", " ", "=", 
          RowBox[{"Simplify", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"(", 
              RowBox[{"Sin", "[", "\[Theta]", "]"}], ")"}], "^", 
             RowBox[{"(", 
              RowBox[{"2", "*", "S"}], ")"}]}], "*", "output"}], "]"}]}], ";",
          "\[IndentingNewLine]", 
         RowBox[{"For", "[", 
          RowBox[{
           RowBox[{"i", "=", "1"}], ",", 
           RowBox[{"1", "<=", "i", "<=", 
            RowBox[{"2", "*", "S"}]}], ",", 
           RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"output", "=", 
             RowBox[{"Simplify", "[", 
              RowBox[{
               RowBox[{"(", 
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{
                   RowBox[{"L0dag", "[", "output", "]"}], "+", 
                   RowBox[{"S", "*", 
                    RowBox[{"Cot", "[", "\[Theta]", "]"}], "*", "output"}]}], 
                  ")"}], "/", 
                 RowBox[{"Sin", "[", "\[Theta]", "]"}]}], ")"}], "/.", 
               "AngTeukPOSRule"}], "]"}]}], ";"}]}], "\[IndentingNewLine]", 
          "]"}], ";", "\[IndentingNewLine]", 
         RowBox[{"output", "=", 
          RowBox[{"Simplify", "[", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{"(", 
               RowBox[{"Sin", "[", "\[Theta]", "]"}], ")"}], "^", 
              RowBox[{"(", 
               RowBox[{"2", "*", "S"}], ")"}]}], "*", "output"}], "/.", 
            RowBox[{"s", "->", "S"}]}], "]"}]}], ";"}], "\[IndentingNewLine]",
         "}"}], ",", "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"For", "[", 
          RowBox[{
           RowBox[{"i", "=", "1"}], ",", 
           RowBox[{"1", "<=", "i", "<=", 
            RowBox[{"2", "*", 
             RowBox[{"(", 
              RowBox[{"-", "S"}], ")"}]}]}], ",", 
           RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"output", "=", 
             RowBox[{"Simplify", "[", 
              RowBox[{
               RowBox[{"(", 
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{
                   RowBox[{"L0dag", "[", "output", "]"}], "+", 
                   RowBox[{
                    RowBox[{"(", 
                    RowBox[{"-", "S"}], ")"}], "*", 
                    RowBox[{"Cot", "[", "\[Theta]", "]"}], "*", "output"}]}], 
                  ")"}], "/", 
                 RowBox[{"Sin", "[", "\[Theta]", "]"}]}], ")"}], "/.", 
               "AngTeukNEGRule"}], "]"}]}], ";"}]}], "\[IndentingNewLine]", 
          "]"}], ";", "\[IndentingNewLine]", 
         RowBox[{"output", " ", "=", 
          RowBox[{"Simplify", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"(", 
              RowBox[{"Sin", "[", "\[Theta]", "]"}], ")"}], "^", 
             RowBox[{"(", 
              RowBox[{"2", "*", 
               RowBox[{"(", 
                RowBox[{"-", "S"}], ")"}]}], ")"}]}], "*", "output"}], 
           "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"For", "[", 
          RowBox[{
           RowBox[{"i", "=", "1"}], ",", 
           RowBox[{"1", "<=", "i", "<=", 
