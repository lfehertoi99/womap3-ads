     "     ", 
     RowBox[{"F2", " ", "=", " ", 
      RowBox[{"Function", "[", 
       RowBox[{
        RowBox[{"{", "r", "}"}], ",", " ", "F2expr"}], "]"}]}], ";", "\n", 
     "\t", "\n", "\t ", 
     RowBox[{"F1coeffs", " ", "=", " ", 
      RowBox[{"Table", "[", 
       RowBox[{
