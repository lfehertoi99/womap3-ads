     RowBox[{"polyVar", " ", "=", " ", 
      RowBox[{"Unique", "[", "x", "]"}]}], ";", "\n", "\t", 
     RowBox[{"poly", " ", "=", " ", 
      RowBox[{"Sum", "[", 
