Cell[BoxData[{
 RowBox[{
  RowBox[{"ClearAll", "[", "CreateExpansion", "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"CreateExpansion", "[", 
   RowBox[{
   "A0_", ",", " ", "maxOrder_Integer", ",", " ", "F1expr_", ",", " ", 
    "F2expr_", ",", " ", "r_", ",", " ", "r0_"}], "]"}], " ", ":=", " ", "\n",
   "\t", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"A", ",", " ", "F1", ",", " ", "F2", ",", " ", "indicial"}], 
     "}"}], ",", "\n", "\t ", 
    RowBox[{
     RowBox[{"F1", " ", "=", " ", 
      RowBox[{"Function", "[", 
       RowBox[{
        RowBox[{"{", "r", "}"}], ",", " ", "F1expr"}], "]"}]}], ";", "\n", 
     "     ", 
     RowBox[{"F2", " ", "=", " ", 
      RowBox[{"Function", "[", 
       RowBox[{
        RowBox[{"{", "r", "}"}], ",", " ", "F2expr"}], "]"}]}], ";", "\n", 
     "\t", "\n", "\t ", 
     RowBox[{"F1coeffs", " ", "=", " ", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"kthCoefficient", "[", 
         RowBox[{"F1", ",", " ", "r", ",", " ", "r0", ",", " ", "n"}], "]"}], 
        ",", " ", 
        RowBox[{"{", 
         RowBox[{"n", ",", " ", "0", ",", " ", "maxOrder"}], "}"}]}], "]"}]}],
      ";", "\n", "     ", 
     RowBox[{"F2coeffs", " ", "=", " ", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"kthCoefficient", "[", 
         RowBox[{"F2", ",", " ", "r", ",", " ", "r0", ",", " ", "n"}], "]"}], 
        ",", " ", 
        RowBox[{"{", 
         RowBox[{"n", ",", " ", "0", ",", " ", "maxOrder"}], "}"}]}], "]"}]}],
      ";", "\n", "     ", "\n", "     ", 
     RowBox[{"coeffs", " ", "=", " ", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"F2coeffs", "[", 
         RowBox[{"[", "1", "]"}], "]"}], ",", " ", 
        RowBox[{
         RowBox[{"F1coeffs", "[", 
          RowBox[{"[", "1", "]"}], "]"}], "-", "1"}], ",", " ", "1"}], 
       "}"}]}], ";", "\n", "     ", 
     RowBox[{"polyVar", " ", "=", " ", 
      RowBox[{"Unique", "[", "x", "]"}]}], ";", "\n", "\t", 
     RowBox[{"poly", " ", "=", " ", 
      RowBox[{"Sum", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"coeffs", "[", 
          RowBox[{"[", 
           RowBox[{"k", "+", "1"}], "]"}], "]"}], " ", 
         RowBox[{"polyVar", "^", "k"}]}], ",", " ", 
        RowBox[{"{", 
         RowBox[{"k", ",", " ", "0", ",", " ", "2"}], "}"}]}], "]"}]}], ";", 
     "\n", "\t", 
     RowBox[{"root1", " ", "=", " ", 
      RowBox[{"polyVar", " ", "/.", " ", 
       RowBox[{
        RowBox[{"Solve", "[", 
         RowBox[{
          RowBox[{"poly", " ", "==", " ", "0"}], ",", " ", "polyVar"}], "]"}],
         "[", 
        RowBox[{"[", "1", "]"}], "]"}]}]}], ";", "\n", "\t", 
     RowBox[{"root2", " ", "=", " ", 
      RowBox[{"polyVar", " ", "/.", " ", 
       RowBox[{
        RowBox[{"Solve", "[", 
         RowBox[{
          RowBox[{"poly", " ", "==", " ", "0"}], ",", " ", "polyVar"}], "]"}],
         "[", 
        RowBox[{"[", "2", "]"}], "]"}]}]}], ";", "\n", "\t", 
     RowBox[{"indicial", " ", "=", " ", 
      RowBox[{"Function", "[", 
       RowBox[{"n", ",", " ", 
        RowBox[{"Evaluate", "[", 
         RowBox[{"poly", " ", "/.", " ", 
          RowBox[{"polyVar", " ", "->", " ", "n"}]}], "]"}]}], "]"}]}], ";", 
     "\n", "   ", "\n", "     ", "\n", "\t ", 
     RowBox[{"A", " ", "=", " ", 
      RowBox[{"ConstantArray", "[", 
       RowBox[{"0", ",", " ", 
        RowBox[{"maxOrder", " ", "+", " ", "1"}]}], "]"}]}], ";", "\n", 
     "     ", 
     RowBox[{
      RowBox[{"A", "[", 
       RowBox[{"[", "1", "]"}], "]"}], " ", "=", " ", "A0"}], ";", "  ", "\n",
      "\n", "  ", 
     RowBox[{"(*", " ", 
      RowBox[{"Recursively", " ", "compute", " ", "coefficients"}], " ", 
      "*)"}], "\n", "  ", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"k", " ", "=", " ", "2"}], ",", " ", 
       RowBox[{"k", " ", "<=", " ", 
        RowBox[{"maxOrder", " ", "+", " ", "1"}]}], ",", " ", 
       RowBox[{"k", "++"}], ",", "\n", "    ", 
       RowBox[{
        RowBox[{
         RowBox[{"A", "[", 
          RowBox[{"[", "k", "]"}], "]"}], " ", "=", " ", 
         RowBox[{
          RowBox[{"-", 
           RowBox[{"Sum", "[", "\n", "       ", 
            RowBox[{
             RowBox[{
              RowBox[{"(", 
               RowBox[{
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"root1", " ", "+", " ", "i"}], ")"}], " ", 
                 RowBox[{"F2coeffs", "[", 
                  RowBox[{"[", 
                   RowBox[{"k", " ", "-", " ", "i"}], "]"}], "]"}]}], " ", "+",
                 " ", 
                RowBox[{"F1coeffs", "[", 
                 RowBox[{"[", 
                  RowBox[{"k", " ", "-", " ", "i"}], "]"}], "]"}]}], ")"}], "*", 
              RowBox[{"A", "[", 
               RowBox[{"[", 
