    RowBox[{"Table", ":", " ", 
     RowBox[{
     "entries", " ", "generated", " ", "from", " ", "i", " ", "and", " ", 
      "j"}]}], "*)"}], "\n", 
   RowBox[{"C", " ", "=", " ", 
    RowBox[{"Table", "[", 
